   { (exit 1); exit 1; }; }
			fi
		else
			SH_CFLAGS="-fPIC"
			SH_LDFLAGS="-shared"
		fi
	else
		# this works for Solaris and IRIX64
		SH_CFLAGS="-KPIC"
		SH_LDFLAGS="-G"
	fi
fi

